{
    "name": "<%= projectName %>",
    "version": "1.0.0",
    "description": "<%= projectDesc %>",
    "repository": {
        "type": "git",
        "url": "<%= githubRepoUrl%>"
    },
    "license": "<%= license %>",
    "author": "<%= githubUsername %> <<%= email %>>",
    "scripts": {
        "preinstall": "npm i -g rimraf mocha",
        "test": "mocha",
        "clear": "rimraf build/ demo/ src/ test/ .eslintrc .gitignore webpack.config.js .yo-rc.json package.json",
        "start": "npm run demo && webpack-dev-server",
        "demo": "rimraf demo/* && webpack",
        "build": "rimraf build/* && webpack"
    },
    "devDependencies": {
        "babel-core": "^6.4.5",
        "babel-loader": "^6.2.2",
        "babel-preset-es2015": "^6.3.13",
        "babel-preset-react": "^6.3.13",
        "webpack": "^1.12.13",
        "webpack-dev-server": "^1.14.1",
        "webpack-merge": "^0.7.3",
        "html-webpack-plugin": "^2.8.1",
        "eslint": "^1.10.3",
        "eslint-loader": "^1.2.1",
        "style-loader": "^0.13.0",
        "css-loader": "^0.23.1",
        "chai": "^3.5.0"
    }
}
